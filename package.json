	"scripts": {
		"nicolehasballsofsteel": "DEBUG=Eleventy* npx eleventy   --config ./config/eleventy.config.js",
		"launch": "npx eleventy   --config ./config/eleventy.config.js --serve",
		"preinstall": "npx npm-force-resolutions",
		"prestart": "npm run clean",
		"start": "NODE_ENV=development npm-run-all dev:webpack dev:server",
		"dev:webpack": "npx webpack --config ./build/webpack/webpack.dev.config.js",
		"dev:server": "npm-run-all --parallel dev:webpack-server dev:eleventy",
		"dev:webpack-server": "webpack-dev-server --config ./build/webpack/webpack.dev.config.js",
		"dev:eleventy": "npx eleventy --config ./config/eleventy.config.js --watch",
		"build": "NODE_ENV=production npm-run-all -s clean build:*",
		"build:webpack": "NODE_ENV=production npx webpack --config ./build/webpack/webpack.prod.config.js",
		"build:eleventy": "NODE_ENV=production npx eleventy --config ./config/eleventy.config.js",
		"clean": "rm -rf ./dist",
		"update": "npx ncu -u; npm install",
		"update-check": "npx npm-check-updates",
		"serve": "npx live-server ./dist --port=8089 --host=localhost"
	},
